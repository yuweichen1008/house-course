    "class-validator": "^0.13.2",
    "cloudinary": "^1.32.0",
    "cloudinary-react": "^1.8.1",
    "cookie": "^0.5.0",
    "deepmerge": "^4.2.2",
    "firebase": "^9.11.0",
    "firebase-admin": "^11.1.0",
    "geolib": "^3.3.3",
    "graphql": "^16.6.0",
    "js-cookie": "^3.0.1",
    "next": "12.3.1",
    "postcss": "^8.4.17",
    "postcss-import": "^15.0.0",
    "prisma": "^4.4.0",
